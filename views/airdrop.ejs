<section id="participate">
  <div class="container">
    <div class="row">
      <div class="col-md-2"></div>
      <div class="col-md-8">
        <h2 class="center mt-3">participate</h2>
        <div class="container">
          <form
            method="post"
            action="/"
            class="participate-form"
            id="main-form"
          >
            <label for="email" class="form-label">Email address</label>
            <div class="input-group mb-3">
              <span class="input-group-text"
                ><i class="far fa-envelope"></i
              ></span>
              <input
                type="email"
                class="form-control input"
                id="email"
                name="email"
                placeholder="JohnDoe@anonymous.com"
                aria-describedby="emailHelp"
                required
              />
            </div>
            <label for="wallet" class="form-label">Wallet: </label>
            <div class="input-group mb-3">
              <span class="input-group-text"
                ><i class="fas fa-wallet"></i
              ></span>
              <input
                type="text"
                name="wallet"
                id="wallet"
                class="form-control input wallet"
                placeholder="BSC Address"
                pattern="0x+[A-F, a-f, 0-9]{40}"
                required
              />
            </div>
            <label for="twitter" class="form-label">Twitter: </label>
            <div class="input-group mb-3">
              <span class="input-group-text"
                ><i class="fab fa-twitter"></i
              ></span>
              <input
                type="text"
                name="twitter"
                class="form-control input"
                id="twitter"
                placeholder="@JohnDoe"
                pattern="@+[A-Z, a-z, 0-9, _, -]{1,20}"
                required
              />
            </div>
            <label for="telegram" class="form-label">Telegram: </label>
            <div class="input-group mb-3">
              <span class="input-group-text"
                ><i class="fab fa-telegram-plane"></i
              ></span>
              <input
                type="text"
                name="telegram"
                class="form-control input"
                id="telegram"
                placeholder="@JohnDoe"
                pattern="@+[A-Z, a-z, 0-9, _, -]{1,20}"
                required
              />
            </div>
            <div class="mb-3 d-none">
              <label for="referre" class="email-label">Referre: </label>
              <input
                type="text"
                name="referre"
                id="referre"
                class="form-control input"
                value="<%= referre %>"
              />
            </div>
            <div class="btn btn-style form-control participate-submit">
              Submit
            </div>
          </form>
          <div class="airdrop-success-msg mt-4"></div>
        </div>
        <h2 class="center pt-5 pb-2">details</h2>
        <div class="container">
          <form action="/" method="get">
            <label for="wallet" class="form-label">Wallet: </label>
            <div class="input-group mb-3">
              <span class="input-group-text"
                ><i class="fas fa-wallet"></i
              ></span>
              <input
                type="text"
                name="wallet"
                id="wallet"
                class="form-control input wallet-details"
                placeholder="BSC Address"
                pattern="0x+[A-F, a-f, 0-9]{40}"
              />
            </div>

            <button
              type="submit"
              class="btn btn-style form-control participant-search"
            >
              Search
            </button>
          </form>
          <div class="search-success-msg mt-4"></div>
        </div>
      </div>
      <div class="col-md-2"></div>
    </div>
  </div>
  <div class="container">
    <div class="participant-details">
      <div class="row">
        <div class="col-md-6">
          <P>
            Referral Link:
            <span id="text_to_copy" class="lowercase the-ref"
              >https://airdrop.kiwanoswap.io/<%= refLink %></span
            ><span class="copy-button"
              ><button id="copy_button" class="btn btn-copy-style">
                Copy
              </button></span
            ></P
          >
        </div>
        <div class="col-md-3">
          <p>refered: <span class="referred"><%= refNum.length %></span></p>
        </div>
        <div class="col-md-3">
          <p>
            balance:
            <span class="balance"><%= refNum.length * 10 + balance %></span>
            WANO
          </p>
        </div>
      </div>
    </div>
  </div>
  <div class="container pt-5">
    <div class="row center">
      <div class="col-6">
        <button
          class="btn btn-secondary btn-style"
          href="https://kiwanoswap.io"
        >
          TRADE WANO TOKEN!
        </button>
      </div>
      <div class="col-6">
        <button
          class="btn btn-secondary btn-style"
          href="https://kiwanoproject.com/buy"
        >
          BUY WANO TOKEN!
        </button>
      </div>
    </div>
  </div>
</section>
